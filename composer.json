{
    "name": "p3d-legacy/pokemon3d.net",
    "type": "project",
    "description": "",
    "keywords": [
        "website",
        "skin",
        "gamejolt",
        "pokemon3d",
        "laravel"
    ],
    "license": "gpl-3.0-only",
    "repositories": {
        "nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    },
    "require": {
        "php": "^8.1",
        "ext-gd": "*",
        "akaunting/laravel-language": "^1.0",
        "alibayat/laravel-categorizable": "dev-master",
        "alibayat/laravel-commentable": "dev-master",
        "anlutro/l4-settings": "^1.3",
        "assada/laravel-achievements": "^2.3",
        "betterapp/laravel-db-encrypter": "^1.0",
        "beyondcode/laravel-favicon": "^1.5.0",
        "blade-ui-kit/blade-ui-kit": "^0.4",
        "bolechen/nova-activitylog": "^0.4.0",
        "cebe/php-openapi": "^1.7",
        "cyrildewit/eloquent-viewable": "^7.0",
        "dedoc/scramble": "^0.8.2",
        "digikraaft/laravel-review-rating": "^3.0",
        "doctrine/dbal": "^3.5",
        "gabrielelana/byte-units": "^0.5.0",
        "glorand/laravel-model-settings": "^6.0",
        "goldspecdigital/laravel-eloquent-uuid": "^10.0",
        "graham-campbell/markdown": "^15.0",
        "guzzlehttp/guzzle": "^7.2",
        "guzzlehttp/psr7": "^2.1",
        "harrk/gamejolt-api": "^1.1",
        "intervention/image": "^2.7",
        "kilobyteno/laravel-plausible": "^1.1.0",
        "kiritokatklian/nova-permission": "^4.0",
        "kkomelin/laravel-translatable-string-exporter": "^1.19",
        "knuckleswtf/scribe": "^4.21.2",
        "laravel/framework": "^10.35",
        "laravel/jetstream": "^4.0",
        "laravel/nova": "4.23.0",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.8",
        "league/html-to-markdown": "^5.1",
        "livewire/livewire": "^3.0",
        "martian/spammailchecker": "^2.0",
        "martinbean/socialite-discord-provider": "^1.2",
        "michael-rubel/laravel-formatters": "^7.0",
        "mtownsend/read-time": "^2.0",
        "ohdearapp/ohdear-php-sdk": "^3.5",
        "origami/consent": "^2.2",
        "overtrue/laravel-like": "^5.1",
        "propaganistas/laravel-disposable-email": "^2.2",
        "restcord/restcord": "dev-develop",
        "sentry/sentry-laravel": "^3.8",
        "socialiteproviders/facebook": "^4.1",
        "socialiteproviders/twitch": "^5.3",
        "spatie/cpu-load-health-check": "^1.0",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-health": "^1.19",
        "spatie/laravel-medialibrary": "^10.7",
        "spatie/laravel-permission": "^5.8",
        "spatie/laravel-schedule-monitor": "^3.2",
        "spatie/laravel-stats": "^2.1",
        "spatie/laravel-tags": "^4.3",
        "spatie/nova-backup-tool": "^5.0",
        "spatie/nova-tags-field": "^4.0",
        "symfony/http-client": "^6.2",
        "symfony/postmark-mailer": "^6.2",
        "torann/geoip": "^3.0",
        "touhidurabir/laravel-multi-key-route-binding": "^1.0",
        "tumblr/tumblr": "^0.4.1",
        "wire-elements/modal": "^2.0",
        "wire-elements/spotlight": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.9.1",
        "laravel-lang/lang": "^13.0",
        "laravel-lang/publisher": "^15.0",
        "laravel/pint": "^1.0.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "treeware/plant": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan nova:publish"
        ]
    }
}
